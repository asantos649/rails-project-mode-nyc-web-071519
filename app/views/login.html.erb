<h1>Login Page</h1>

<p><%= @error %></p>

<%= form_tag("/login") do %>
  <%= label_tag(:email, "Email:") %>
  <%= text_field_tag(:email) %><br>
  <%= label_tag(:password, "Password:") %>
  <%= password_field_tag(:password) %><br>
  <%= submit_tag "Log In" %>
<% end %>

<%= button_to "Sign Up", new_user_path, method: "get" %>